-- arithmeticInterpreter.byr

--With                    -- To compile in Ada
--Ada.Text_IO;            -- To compile in Ada

procedure main is
  c : Character;
begin

  Put("Enter a letter(n to exit): ");
  Get(c);

  while (c /= 'n') loop
    Put("Enter a letter(n to exit): ");
    Get(c);
  end loop;
  Put("You're out!");
  New_Line;
  null;
end main;